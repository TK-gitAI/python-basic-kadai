        "def calculate_total_price(price, tax_rate):\n",
        "\n",
        "    tax_amount = price * (tax_rate / 100)\n",
        "    total_price = price + tax_amount\n",
        "    return total_price\n",
        "\n",
        "price = 100\n",
        "tax_rate = 10\n",
        "total = calculate_total_price(price, tax_rate)\n",
        "print(f\"{int(total)} 円\")\n"
